{
    "name": "mharj-etag-tools",
    "version": "0.0.3",
    "description": "etag object tools",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run test && npm run build",
        "test": "nyc mocha"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/mharj-etag-tools.git"
    },
    "keywords": [
        "etag"
    ],
    "mocha": {
        "recursive": true,
        "exit": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "extension": [
            "ts",
            "js"
        ],
        "reporters": [
            "spec"
        ]
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src"
        ],
        "reporter": [
            "text",
            "cobertura"
        ],
        "all": true
    },
    "files": [
        "dist"
    ],
    "author": "mharj",
    "license": "LGPL-2.1-or-later",
    "bugs": {
        "url": "https://github.com/mharj/mharj-etag-tools/issues"
    },
    "homepage": "https://github.com/mharj/mharj-etag-tools#readme",
    "devDependencies": {
        "@types/chai-as-promised": "^7.1.5",
        "@types/mocha": "^9.1.0",
        "@types/node": "^16.11.26",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "cross-fetch": "^3.1.5",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-sonarjs": "^0.13.0",
        "mocha": "^9.2.2",
        "nyc": "^15.1.0",
        "prettier": "^2.6.2",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    }
}
